<h1>Add a new image now!!</h1>
<%= link_to "Back to imagez", images_url %>

<%= form_for @image do |f| %>
  <p>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :artist %>
    <%= f.text_field :artist %>
  </p>
  <p>
    <%= f.label :description %>
    <%= f.text_field :description %>
  </p>
  <p>
    <%= f.label :url %>
    <%= f.text_field :url %>
  </p>
  <%= f.submit "Save it!!" %>
<% end %>

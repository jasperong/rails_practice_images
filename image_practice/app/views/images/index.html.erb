<h1>I haz so many imagessssss!!!!</h1>
<%= link_to "Put your image in me plzz", new_image_path %>

<ul>
<% @images.reverse.each do |image| %>
  <li>
    <%= link_to image_tag(image.url), "/images/#{image.id}" %><br>
    <%= image.title %><br>
    <%= image.artist %><br>
    <%= image.description %>
  </li>
<% end %>
</ul>
